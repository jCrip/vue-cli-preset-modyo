{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-unit-jest": {},
    "vue-cli-plugin-i18n": {
